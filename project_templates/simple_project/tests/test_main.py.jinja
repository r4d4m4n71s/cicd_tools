"""
Tests for the sample module.
"""

import unittest

from sample_module.main import process_data


class TestSampleModule(unittest.TestCase):
    """Tests for the sample module."""
    
    def test_process_data(self):
        """Test the process_data function."""
        self.assertEqual(process_data([1, 2, 3, 4, 5]), 15)
        self.assertEqual(process_data([10, 20]), 30)


if __name__ == "__main__":
    unittest.main()